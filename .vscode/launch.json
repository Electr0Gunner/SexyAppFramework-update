      "program": "${workspaceFolder}/examples/bin/V14Demo",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/examples/bin/",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": "${workspaceFolder}/examples/bin:${env:LD_LIBRARY_PATH}"
        }
      ],
      "setupCommands": [
        {
          "description": "Enable pretty-printing for GDB",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
